{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "probe-rs-debug",
            "request": "launch",
            "name": "probe-rs Test",
            "cwd": "${workspaceFolder}",
            "connectUnderReset": false,
            "chip": "ATSAMD21G18A",
            "flashingConfig": {
                "flashingEnabled": true,
                "haltAfterReset": true
