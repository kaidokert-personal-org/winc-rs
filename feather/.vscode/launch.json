            "consoleLogLevel": "Debug",
            "coreConfigs": [
                {
                    "rttEnabled": false,
                    "coreIndex": 0,
                    "programBinary": "${workspaceFolder}\\target\\thumbv6m-none-eabi\\debug\\blinky",
                }
            ],
            "env": {
                "RUST_LOG": "debug"
            }
        }
    ]
}